defshell -zsh

screen -t tidal
select 0
stuff "emacs^M"
screen -t jackd
select 1
stuff "sh ~/scripts/jackup.sh^M"
screen -t dirt
select 2
stuff "sh ~/scripts/dirtup.sh^M"
screen -t ffserver
select 3
stuff "ffserver -f ~/ffserver.conf^M"
screen -t ffmpeg
select 4
stuff "sleep 5; ffmpeg -f jack -i ffmpeg -ac 2 http://localhost:8090/feed1.ffm^M"
screen -t files
select 5
stuff "cd /work/scratch^M"
select 0

altscreen off
term screen-256color 
bind ',' prev
bind '.' next
